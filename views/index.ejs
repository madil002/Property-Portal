<!doctype html>
<html>

<head>
    <title>
        <%=appName%>
    </title>
    <link rel="stylesheet" type="text/css" href="/main.css" />
</head>

<body>
    <h1>
        <%=appName%>
    </h1>

    <% if (typeof error !== 'undefined' && error) { %>
        <p class="error-message"><%= error %></p>
    <% } %>
    <% if (typeof success !== 'undefined' && success) { %>
        <p class="success-message"><%= success %></p>
    <% } %>
    
    <p><a href="properties">Our Properties</a></p>
    <p><a href="search">Search Properties</a></p>
    <p><a href="listproperty">List a Property</a></p>
    <p><a href="more-properties">More external Properties</a></p>
    <p><a href="login">Login</a></p>
    <p><a href="register">Register</a></p>
    <p><a href="loggedout">Logout</a></p>
    <p><a href="dashboard">Dashboard</a></p>
    <p><a href="about">About <%=appName%></a></p>
</body>

</html>